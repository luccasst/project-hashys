      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "nodemon": "^2.0.20",
      "pg": "^8.9.0",
      "reflect-metadata": "^0.1.13",
      "ts-node": "^10.9.1",
      "typeorm": "^0.3.12",
